{
  "name": "cope",
  "version": "1.0.0",
  "main": "index.js",
  "scripts": {
    "android": "npx react-native run-android",
    "ios": "npx react-native run-ios",
    "start:dev": "cp .env.development .env",
    "start:staging": "cp .env.staging .env",
    "start:prod": "cp .env.production .env",
    "clean": "rm -rf node_modules && npm cache clean --force",
    "clean:ios": "cd ios && rm -rf Pods Podfile.lock build && cd ..",
    "clean:android": "cd android && ./gradlew clean && cd ..",
    "clean:all": "npm run clean && npm run clean:ios && npm run clean:android"
  },
  "dependencies": {
    "@react-native-async-storage/async-storage": "^2.1.2",
    "@react-navigation/bottom-tabs": "^7.3.10",
    "@react-navigation/native": "^7.1.6",
    "@react-navigation/stack": "^7.2.10",
    "@supabase/supabase-js": "^2.49.4",
    "react": "18.3.1",
    "react-native": "0.76.9",
    "react-native-chart-kit": "^6.12.0",
    "react-native-echarts-wrapper": "^2.0.0",
    "react-native-gesture-handler": "^2.25.0",
    "react-native-push-notification": "^8.1.1",
    "react-native-reanimated": "^3.17.4",
    "react-native-safe-area-context": "^5.4.0",
    "react-native-screens": "^4.10.0",
    "react-native-svg": "^15.11.2",
    "react-native-ui-lib": "^7.40.1",
    "react-native-url-polyfill": "^2.0.0",
    "react-native-vector-icons": "^10.2.0"
  },
  "devDependencies": {
    "@babel/core": "^7.25.2",
    "@babel/preset-env": "^7.25.3",
    "@babel/runtime": "^7.25.0",
    "@react-native-community/cli": "latest",
    "@react-native-community/cli-platform-android": "latest",
    "@react-native-community/cli-platform-ios": "latest",
    "@react-native/babel-preset": "^0.75.0",
    "@react-native/eslint-config": "^0.75.0",
    "@react-native/typescript-config": "^0.75.0",
    "@types/react": "~18.3.12",
    "@types/react-native-push-notification": "^8.1.4",
    "@types/react-native-vector-icons": "^6.4.18",
    "eslint": "^8.19.0",
    "prettier": "2.8.8",
    "react-native-dotenv": "^3.4.9",
    "typescript": "^5.3.3"
  },
  "private": true
}
